					<box_checked2>PowerBrowse</box_checked2>
					<box_checked3>FEU</box_checked3>
					<box_checked4>NMS</box_checked4>
					<box_checked5>Cataloger</box_checked5>
					<box_checked6>Other(s)</box_checked6>
