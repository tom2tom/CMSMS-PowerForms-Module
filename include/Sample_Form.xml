			<field>
				<properties>
					<field_id>1</field_id>
					<name>Fieldset Start</name>
					<type>FieldsetStart</type>
					<order_by>2</order_by>
				</properties>
				<options>
					<legend>Help Improve Power Forms</legend>
					<alias>fieldset_start</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>2</field_id>
					<name>Send It</name>
					<type>DispositionEmail</type>
					<order_by>3</order_by>
				</properties>
				<options>
					<email_subject>PowerForms Sample Form Submission</email_subject>
					<email_from_name>Power Forms Installation</email_from_name>
					<email_from_address>me@overhere.com</email_from_address>
					<destination_address>you@overthere.net</destination_address>
					<email_template>TODO</email_template>
					<email_encoding>utf-8</email_encoding>
					<html_email>1</html_email>
					<alias>send_it</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>3</field_id>
					<name>Welcome!</name>
					<type>StaticTextField</type>
					<order_by>4</order_by>
				</properties>
				<options>
					<text>Thanks for trying out PowerForms. You can use this form to provide feedback about how you will be using this module.</text>
					<css_class>message</css_class>
					<alias>welcome</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>4</field_id>
					<name>Modules you'll be using together</name>
					<type>CheckboxGroupField</type>
					<order_by>5</order_by>
				</properties>
				<options>
					<box_name1>PowerForms</box_name1>
					<box_name2>PowerBrowse</box_name2>
					<box_name3>Front End Users</box_name3>
					<box_name4>Newsletter Made Simple (NMS)</box_name4>
					<box_name5>Cataloger</box_name5>
					<box_name6>Other(s)</box_name6>
