<?xml version="1.0" standalone="yes"?>
<!DOCTYPE powerforms [
<!ELEMENT powerforms (version,date,count,form)>
<!ELEMENT version (#PCDATA)>
<!ELEMENT date (#PCDATA)>
<!ELEMENT count (#PCDATA)>
<!ELEMENT form (properties,options?,fields?)>
<!ELEMENT properties (#PCDATA)>
<!ELEMENT options (#PCDATA)>
<!ELEMENT fields (field)>
<!ELEMENT field (properties,options?)>
]>
<powerforms>
	<version>0.7</version>
	<date></date>
	<count>1</count>
	<form>
		<properties>
			<form_id>1</form_id>
			<name>Advanced Contact Form</name>
			<alias>advanced_contact</alias>
		</properties>
		<options>
			<captcha_wrong>The entered text was not correct.</captcha_wrong>
			<css_class>advancedcontactform</css_class>
			<form_template>TODO</form_template>
			<list_delimiter>,</list_delimiter>
			<next_button_text>Continue...</next_button_text>
			<prev_button_text>Back...</prev_button_text>
			<redirect_page>-1</redirect_page>
			<required_field_symbol>*</required_field_symbol>
			<submission_template>TODO</submission_template>
			<submit_action>text</submit_action>
			<submit_button_text>Submit Form</submit_button_text>
			<title_user_captcha>Help prevent abuse by spammers, enter the text from the image.</title_user_captcha>
			<unspecified>[unspecified]</unspecified>
		</options>
		<fields>
			<field>
				<properties>
					<field_id>1</field_id>
					<name>About You Fieldset</name>
					<type>FieldsetStart</type>
					<order_by>1</order_by>
				</properties>
				<options>
					<legend>About You</legend>
					<alias>about_you_fieldset</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>2</field_id>
					<name>Your Name</name>
					<type>FromEmailNameField</type>
					<order_by>2</order_by>
				</properties>
				<options>
					<field_alias>users_name</field_alias>
					<alias>your_name</alias>
					<required>1</required>
				</options>
			</field>
			<field>
				<properties>
					<field_id>3</field_id>
					<name>Your Email Address</name>
					<type>DispositionFromEmailAddressField</type>
					<order_by>3</order_by>
				</properties>
				<options>
					<email_subject>Email from Site</email_subject>
					<email_from_name>Power Forms</email_from_name>
					<send_user_copy>c</send_user_copy>
					<send_user_label>Send me a copy of form</send_user_label>
					<email_template>TODO</email_template>
					<email_encoding>utf-8</email_encoding>
					<alias>your_email_address</alias>
					<validation_type>none</validation_type>
				</options>
			</field>
			<field>
				<properties>
					<field_id>4</field_id>
					<name>Email Again</name>
					<type>FromEmailAddressAgainField</type>
					<order_by>4</order_by>
				</properties>
				<options>
					<field_to_validate>Your Email Address</field_to_validate>
					<alias>email_again</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>5</field_id>
					<name>About You Fieldset End</name>
					<type>FieldsetEnd</type>
					<order_by>5</order_by>
				</properties>
				<options>
					<alias>about_you_fieldset_end</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>6</field_id>
					<name>breaker</name>
					<type>PageBreakField</type>
					<order_by>6</order_by>
				</properties>
				<options>
					<alias>breaker</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>7</field_id>
					<name>Contact Fieldset</name>
					<type>FieldsetStart</type>
					<order_by>7</order_by>
				</properties>
				<options>
					<legend>Contact</legend>
					<alias>contact_fieldset</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>8</field_id>
					<name>Greeting</name>
					<type>StaticTextField</type>
					<order_by>8</order_by>
				</properties>
				<options>
					<text>Hello, {$users_name->value}!<br /> What is it you'd like to talk to us about?</text>
					<smarty_eval>1</smarty_eval>
					<alias>greeting</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>9</field_id>
					<name>Subject</name>
					<type>FromEmailSubjectField</type>
					<order_by>9</order_by>
				</properties>
				<options>
					<alias>subject</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>10</field_id>
					<name>Message</name>
					<type>TextAreaField</type>
					<order_by>10</order_by>
				</properties>
				<options>
					<rows>5</rows>
					<cols>60</cols>
					<field_alias>message</field_alias>
					<default>Enter Your Message Here</default>
					<clear_default>1</clear_default>
					<alias>message</alias>
					<required>1</required>
				</options>
			</field>
			<field>
				<properties>
					<field_id>11</field_id>
					<name>Send To</name>
					<type>DispositionEmail</type>
					<order_by>11</order_by>
				</properties>
				<options>
					<email_subject>Contact Form Submission</email_subject>
					<email_from_name>Power Forms</email_from_name>
					<email_from_address>me@overhere.com</email_from_address>
					<destination_address>you@overthere.net</destination_address>
					<html_email>1</html_email>
					<email_template>TODO</email_template>
					<email_encoding>utf-8</email_encoding>
					<alias>send_to</alias>
				</options>
			</field>
			<field>
				<properties>
					<field_id>12</field_id>
					<name>Contact Fieldset End</name>
					<type>FieldsetEnd</type>
					<order_by>12</order_by>
				</properties>
				<options>
					<alias>contact_fieldset_end</alias>
				</options>
			</field>
		</fields>
	</form>
</powerforms>
