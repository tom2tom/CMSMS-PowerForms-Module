<?xml version="1.0" standalone="yes"?>
<!DOCTYPE powerforms [
<!ELEMENT powerforms (version,date,count,form)>
<!ELEMENT version (#PCDATA)>
<!ELEMENT date (#PCDATA)>
<!ELEMENT count (#PCDATA)>
<!ELEMENT form (properties,attributes,fields,fieldopts)>
<!ELEMENT properties (form_id,name,alias)>
<!ELEMENT form_id (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT alias (#PCDATA)>
<!ELEMENT attributes (attribute)>
<!ELEMENT attribute (form_attr_id,name,value)>
<!ELEMENT form_attr_id (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT value (#PCDATA)>
<!ELEMENT fields (field)>
<!ELEMENT field (field_id,name,type,validation_type,required,hide_label,order_by,option)>
<!ELEMENT field_id (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT type (#PCDATA)>
<!ELEMENT validation_type (#PCDATA)>
<!ELEMENT required (#PCDATA)>
<!ELEMENT hide_label (#PCDATA)>
<!ELEMENT order_by (#PCDATA)>
<!ELEMENT option (option_id,name,value)>
<!ELEMENT option_id (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT value (#PCDATA)>
]>
