<!ELEMENT form (properties,attributes,fields,fieldopts)>
<!ELEMENT properties (form_id,name,alias)>
<!ELEMENT form_id (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT alias (#PCDATA)>
<!ELEMENT attributes (attribute)>
<!ELEMENT attribute (form_attr_id,name,value)>
<!ELEMENT form_attr_id (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT value (#PCDATA)>
<!ELEMENT fields (field)>
<!ELEMENT field (field_id,name,alias,type,order_by,option)>
